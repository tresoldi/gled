        </prior>
        <prior id="clockPrior:default" name="distribution" x="@clockRate.c:default">
          <Uniform id="UniformClockPrior:default" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_alpha_prior.s:chibchan" name="distribution" x="@covarion_alpha.s:chibchan">
          <Uniform id="CovAlphaUniform:chibchan" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_s_prior.s:chibchan" name="distribution" x="@covarion_s.s:chibchan">
          <Gamma id="Gamma.0:chibchan" name="distr">
            <parameter id="covarion_switch_gamma_param1:chibchan" name="alpha" lower="0.0" upper="0.0">0.05</parameter>
            <parameter id="covarion_switch_gamma_param2:chibchan" name="beta" lower="0.0" upper="0.0">10.0</parameter>
          </Gamma>
        </prior>
      </distribution>
      <distribution id="likelihood" spec="util.CompoundDistribution">
        <distribution id="DataLikelihood:chibchan" spec="TreeLikelihood" useAmbiguities="true" branchRateModel="@StrictClockModel.c:default" tree="@Tree.t:beastlingTree">
          <siteModel id="SiteModel.chibchan" spec="SiteModel" mutationRate="1.0" proportionInvariant="0">
            <substModel id="covarion.s:chibchan" spec="BinaryCovarion" alpha="@covarion_alpha.s:chibchan" switchRate="@covarion_s.s:chibchan">
              <vfrequencies id="chibchan:visiblefrequencies.s" dimension="2" spec="parameter.RealParameter">0.91 0.09</vfrequencies>
              <parameter id="chibchan:hiddenfrequencies.s" dimension="2" lower="0.0" name="hfrequencies" upper="1.0">0.5 0.5</parameter>
              <frequencies id="chibchan:dummyfrequences.s" spec="Frequencies" frequencies="0.5 0.5" />
            </substModel>
          </siteModel>
          <data id="filtered_data_chibchan" spec="FilteredAlignment" data="@data_chibchan" filter="-" ascertained="true" excludefrom="0" excludeto="1">
