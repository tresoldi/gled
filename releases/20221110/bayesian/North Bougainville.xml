        <prior id="clockPrior:default" name="distribution" x="@clockRate.c:default">
          <Uniform id="UniformClockPrior:default" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_alpha_prior.s:northbougainville" name="distribution" x="@covarion_alpha.s:northbougainville">
          <Uniform id="CovAlphaUniform:northbougainville" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_s_prior.s:northbougainville" name="distribution" x="@covarion_s.s:northbougainville">
          <Gamma id="Gamma.0:northbougainville" name="distr">
            <parameter id="covarion_switch_gamma_param1:northbougainville" name="alpha" lower="0.0" upper="0.0">0.05</parameter>
            <parameter id="covarion_switch_gamma_param2:northbougainville" name="beta" lower="0.0" upper="0.0">10.0</parameter>
          </Gamma>
        </prior>
      </distribution>
      <distribution id="likelihood" spec="util.CompoundDistribution">
        <distribution id="DataLikelihood:northbougainville" spec="TreeLikelihood" useAmbiguities="true" branchRateModel="@StrictClockModel.c:default" tree="@Tree.t:beastlingTree">
