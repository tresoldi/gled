    <sequence id="language_data_northbougainville:rapo1238" taxon="rapo1238" value="0101010101001001000100010001001001000010010001001001000100100100100010101001001000??0100100100100100101" />
    <sequence id="language_data_northbougainville:roto1249" taxon="roto1249" value="0010001000100100110011001100100100111001001100100100110010010010011001000100100110110010010010010010010" />
  </data>
  <branchRateModel clock.rate="@clockRate.c:default" id="StrictClockModel.c:default" spec="beast.evolution.branchratemodel.StrictClockModel" />
  <run id="mcmc" spec="MCMC" chainLength="10000000" numInitializationAttempts="1000" sampleFromPrior="false">
    <state id="state" storeEvery="5000">
      <tree id="Tree.t:beastlingTree" name="stateNode">
        <taxonset idref="taxa" />
