            <substModel id="covarion.s:northbougainville" spec="BinaryCovarion" alpha="@covarion_alpha.s:northbougainville" switchRate="@covarion_s.s:northbougainville">
              <vfrequencies id="northbougainville:visiblefrequencies.s" dimension="2" spec="parameter.RealParameter">0.65 0.35</vfrequencies>
              <parameter id="northbougainville:hiddenfrequencies.s" dimension="2" lower="0.0" name="hfrequencies" upper="1.0">0.5 0.5</parameter>
              <frequencies id="northbougainville:dummyfrequences.s" spec="Frequencies" frequencies="0.5 0.5" />
            </substModel>
          </siteModel>
          <data id="filtered_data_northbougainville" spec="FilteredAlignment" data="@data_northbougainville" filter="-" ascertained="true" excludefrom="0" excludeto="1">
            <userDataType id="TwoStateCovarionDatatype" spec="beast.evolution.datatype.TwoStateCovarion" />
          </data>
        </distribution>
      </distribution>
    </distribution>
    <operator id="UpDown" spec="UpDownOperator" scaleFactor="0.5" weight="3.0">
      <tree idref="Tree.t:beastlingTree" name="up" />
      <parameter idref="birthRate.t:beastlingTree" name="down" />
    </operator>
    <operator id="YuleBirthRateScaler.t:beastlingTree" spec="ScaleOperator" parameter="@birthRate.t:beastlingTree" scaleFactor="0.5" weight="3.0" />
    <operator id="covarion_alpha_scaler.s:northbougainville" spec="ScaleOperator" parameter="@covarion_alpha.s:northbougainville" scaleFactor="0.5" weight="1.0" />
    <operator id="northbougainville:covarion_s_scaler.s" spec="ScaleOperator" parameter="@covarion_s.s:northbougainville" scaleFactor="0.5" weight="1.0" />
    <logger id="screenlog" logEvery="1000">
      <log arg="@posterior" id="ESS.0" spec="util.ESS" />
      <log idref="prior" />
      <log idref="likelihood" />
      <log idref="posterior" />
    </logger>
    <logger id="tracelog" fileName="northbougainville.log" logEvery="1000" sort="smart">
      <log idref="prior" />
      <log idref="likelihood" />
      <log idref="posterior" />
