      <parameter id="covarion_alpha.s:northbougainville" lower="1.0E-4" name="stateNode" upper="1.0">0.5</parameter>
      <parameter id="covarion_s.s:northbougainville" lower="1.0E-4" name="stateNode" upper="Infinity">0.5</parameter>
