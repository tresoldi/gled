            <parameter id="covarion_switch_gamma_param1:nubian" name="alpha" lower="0.0" upper="0.0">0.05</parameter>
            <parameter id="covarion_switch_gamma_param2:nubian" name="beta" lower="0.0" upper="0.0">10.0</parameter>
          </Gamma>
        </prior>
      </distribution>
      <distribution id="likelihood" spec="util.CompoundDistribution">
        <distribution id="DataLikelihood:nubian" spec="TreeLikelihood" useAmbiguities="true" branchRateModel="@StrictClockModel.c:default" tree="@Tree.t:beastlingTree">
