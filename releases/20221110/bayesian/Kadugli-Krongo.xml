            <parameter id="covarion_switch_gamma_param1:kaduglikrongo" name="alpha" lower="0.0" upper="0.0">0.05</parameter>
            <parameter id="covarion_switch_gamma_param2:kaduglikrongo" name="beta" lower="0.0" upper="0.0">10.0</parameter>
          </Gamma>
        </prior>
