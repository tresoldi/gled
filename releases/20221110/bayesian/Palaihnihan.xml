        <prior id="covarion_s_prior.s:palaihnihan" name="distribution" x="@covarion_s.s:palaihnihan">
          <Gamma id="Gamma.0:palaihnihan" name="distr">
            <parameter id="covarion_switch_gamma_param1:palaihnihan" name="alpha" lower="0.0" upper="0.0">0.05</parameter>
            <parameter id="covarion_switch_gamma_param2:palaihnihan" name="beta" lower="0.0" upper="0.0">10.0</parameter>
