    <operator id="covarion_alpha_scaler.s:palaihnihan" spec="ScaleOperator" parameter="@covarion_alpha.s:palaihnihan" scaleFactor="0.5" weight="1.0" />
    <operator id="palaihnihan:covarion_s_scaler.s" spec="ScaleOperator" parameter="@covarion_s.s:palaihnihan" scaleFactor="0.5" weight="1.0" />
    <logger id="screenlog" logEvery="1000">
