        <distribution id="YuleModel.t:beastlingTree" tree="@Tree.t:beastlingTree" spec="beast.evolution.speciation.YuleModel" birthDiffRate="@birthRate.t:beastlingTree" />
        <prior id="YuleBirthRatePrior.t:beastlingTree" name="distribution" x="@birthRate.t:beastlingTree">
          <Uniform id="Uniform.0" name="distr" upper="Infinity" />
        </prior>
        <prior id="clockPrior:default" name="distribution" x="@clockRate.c:default">
          <Uniform id="UniformClockPrior:default" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_alpha_prior.s:siouan" name="distribution" x="@covarion_alpha.s:siouan">
          <Uniform id="CovAlphaUniform:siouan" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_s_prior.s:siouan" name="distribution" x="@covarion_s.s:siouan">
          <Gamma id="Gamma.0:siouan" name="distr">
            <parameter id="covarion_switch_gamma_param1:siouan" name="alpha" lower="0.0" upper="0.0">0.05</parameter>
            <parameter id="covarion_switch_gamma_param2:siouan" name="beta" lower="0.0" upper="0.0">10.0</parameter>
          </Gamma>
        </prior>
      </distribution>
      <distribution id="likelihood" spec="util.CompoundDistribution">
        <distribution id="DataLikelihood:siouan" spec="TreeLikelihood" useAmbiguities="true" branchRateModel="@StrictClockModel.c:default" tree="@Tree.t:beastlingTree">
          <siteModel id="SiteModel.siouan" spec="SiteModel" mutationRate="1.0" proportionInvariant="0">
            <substModel id="covarion.s:siouan" spec="BinaryCovarion" alpha="@covarion_alpha.s:siouan" switchRate="@covarion_s.s:siouan">
              <vfrequencies id="siouan:visiblefrequencies.s" dimension="2" spec="parameter.RealParameter">0.85 0.15</vfrequencies>
              <parameter id="siouan:hiddenfrequencies.s" dimension="2" lower="0.0" name="hfrequencies" upper="1.0">0.5 0.5</parameter>
              <frequencies id="siouan:dummyfrequences.s" spec="Frequencies" frequencies="0.5 0.5" />
