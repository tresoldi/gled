  <branchRateModel clock.rate="@clockRate.c:default" id="StrictClockModel.c:default" spec="beast.evolution.branchratemodel.StrictClockModel" />
  <run id="mcmc" spec="MCMC" chainLength="10000000" numInitializationAttempts="1000" sampleFromPrior="false">
    <state id="state" storeEvery="5000">
      <tree id="Tree.t:beastlingTree" name="stateNode">
        <taxonset idref="taxa" />
      </tree>
      <parameter id="birthRate.t:beastlingTree" name="stateNode">1.0</parameter>
      <parameter id="clockRate.c:default" upper="1000.0" name="stateNode">1.0</parameter>
      <parameter id="covarion_alpha.s:sepik" lower="1.0E-4" name="stateNode" upper="1.0">0.5</parameter>
      <parameter id="covarion_s.s:sepik" lower="1.0E-4" name="stateNode" upper="Infinity">0.5</parameter>
    </state>
    <init estimate="false" id="startingTree" initial="@Tree.t:beastlingTree" taxonset="@taxa" spec="beast.evolution.tree.RandomTree">
      <populationModel spec="ConstantPopulation">
        <popSize spec="parameter.RealParameter" value="1" />
      </populationModel>
    </init>
    <distribution id="posterior" spec="util.CompoundDistribution">
      <distribution id="prior" spec="util.CompoundDistribution">
        <distribution id="YuleModel.t:beastlingTree" tree="@Tree.t:beastlingTree" spec="beast.evolution.speciation.YuleModel" birthDiffRate="@birthRate.t:beastlingTree" />
        <prior id="YuleBirthRatePrior.t:beastlingTree" name="distribution" x="@birthRate.t:beastlingTree">
          <Uniform id="Uniform.0" name="distr" upper="Infinity" />
        </prior>
        <prior id="clockPrior:default" name="distribution" x="@clockRate.c:default">
          <Uniform id="UniformClockPrior:default" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_alpha_prior.s:sepik" name="distribution" x="@covarion_alpha.s:sepik">
          <Uniform id="CovAlphaUniform:sepik" name="distr" upper="Infinity" />
        </prior>
        <prior id="covarion_s_prior.s:sepik" name="distribution" x="@covarion_s.s:sepik">
          <Gamma id="Gamma.0:sepik" name="distr">
            <parameter id="covarion_switch_gamma_param1:sepik" name="alpha" lower="0.0" upper="0.0">0.05</parameter>
            <parameter id="covarion_switch_gamma_param2:sepik" name="beta" lower="0.0" upper="0.0">10.0</parameter>
          </Gamma>
        </prior>
      </distribution>
      <distribution id="likelihood" spec="util.CompoundDistribution">
        <distribution id="DataLikelihood:sepik" spec="TreeLikelihood" useAmbiguities="true" branchRateModel="@StrictClockModel.c:default" tree="@Tree.t:beastlingTree">
          <siteModel id="SiteModel.sepik" spec="SiteModel" mutationRate="1.0" proportionInvariant="0">
            <substModel id="covarion.s:sepik" spec="BinaryCovarion" alpha="@covarion_alpha.s:sepik" switchRate="@covarion_s.s:sepik">
              <vfrequencies id="sepik:visiblefrequencies.s" dimension="2" spec="parameter.RealParameter">0.94 0.06</vfrequencies>
              <parameter id="sepik:hiddenfrequencies.s" dimension="2" lower="0.0" name="hfrequencies" upper="1.0">0.5 0.5</parameter>
              <frequencies id="sepik:dummyfrequences.s" spec="Frequencies" frequencies="0.5 0.5" />
            </substModel>
          </siteModel>
          <data id="filtered_data_sepik" spec="FilteredAlignment" data="@data_sepik" filter="-" ascertained="true" excludefrom="0" excludeto="1">
            <userDataType id="TwoStateCovarionDatatype" spec="beast.evolution.datatype.TwoStateCovarion" />
          </data>
        </distribution>
      </distribution>
    </distribution>
    <operator id="UpDown" spec="UpDownOperator" scaleFactor="0.5" weight="3.0">
      <tree idref="Tree.t:beastlingTree" name="up" />
      <parameter idref="birthRate.t:beastlingTree" name="down" />
    </operator>
    <operator id="YuleBirthRateScaler.t:beastlingTree" spec="ScaleOperator" parameter="@birthRate.t:beastlingTree" scaleFactor="0.5" weight="3.0" />
    <operator id="covarion_alpha_scaler.s:sepik" spec="ScaleOperator" parameter="@covarion_alpha.s:sepik" scaleFactor="0.5" weight="1.0" />
    <operator id="sepik:covarion_s_scaler.s" spec="ScaleOperator" parameter="@covarion_s.s:sepik" scaleFactor="0.5" weight="1.0" />
    <logger id="screenlog" logEvery="1000">
      <log arg="@posterior" id="ESS.0" spec="util.ESS" />
      <log idref="prior" />
      <log idref="likelihood" />
      <log idref="posterior" />
    </logger>
    <logger id="tracelog" fileName="sepik.log" logEvery="1000" sort="smart">
      <log idref="prior" />
      <log idref="likelihood" />
      <log idref="posterior" />
    </logger>
    <logger mode="tree" fileName="sepik.nex" logEvery="1000" id="treeLogger">
      <log id="TreeLoggerWithMetaData" spec="beast.evolution.tree.TreeWithMetaDataLogger" tree="@Tree.t:beastlingTree" dp="4" />
    </logger>
  </run>
