    <operator id="YuleBirthRateScaler.t:beastlingTree" spec="ScaleOperator" parameter="@birthRate.t:beastlingTree" scaleFactor="0.5" weight="3.0" />
    <operator id="covarion_alpha_scaler.s:yam" spec="ScaleOperator" parameter="@covarion_alpha.s:yam" scaleFactor="0.5" weight="1.0" />
    <operator id="yam:covarion_s_scaler.s" spec="ScaleOperator" parameter="@covarion_s.s:yam" scaleFactor="0.5" weight="1.0" />
    <logger id="screenlog" logEvery="1000">
      <log arg="@posterior" id="ESS.0" spec="util.ESS" />
      <log idref="prior" />
      <log idref="likelihood" />
      <log idref="posterior" />
    </logger>
    <logger id="tracelog" fileName="yam.log" logEvery="1000" sort="smart">
      <log idref="prior" />
      <log idref="likelihood" />
      <log idref="posterior" />
